# FreeSWITCH Voicemail Detection System - Environment Variables
# Copy this file to .env and customize the values for your deployment

# =============================================================================
# NETWORK CONFIGURATION
# =============================================================================

# External IP address for SIP traffic (change to your server's public IP)
FREESWITCH_EXTERNAL_IP=192.168.100.50

# SIP Port for FreeSWITCH
FREESWITCH_SIP_PORT=5060

# Event Socket Port for fs_cli
FREESWITCH_EVENT_SOCKET_PORT=8021

# RTP Port Range for voice traffic
FREESWITCH_RTP_START=16384
FREESWITCH_RTP_END=16500

# =============================================================================
# GATEWAY CONFIGURATION
# =============================================================================

# Outbound gateway for voicemail detection calls
GATEWAY_HOST=172.16.250.197
GATEWAY_PORT=5060
GATEWAY_USERNAME=77751
GATEWAY_PASSWORD=77751
GATEWAY_AUTH_PREFIX=77751

# =============================================================================
# ASR/VOICEMAIL DETECTION
# =============================================================================

# Vosk ASR Server Configuration
VOSK_HOST=vosk-server
VOSK_PORT=2700
VOSK_MODEL=vosk-model-es-0.42

# Detection confidence threshold (70-95 recommended)
DETECTION_CONFIDENCE_THRESHOLD=80

# Detection timeout in seconds
DETECTION_TIMEOUT=30

# =============================================================================
# DATABASE CONFIGURATION (Optional - for advanced logging)
# =============================================================================

# PostgreSQL Database for call logging and pattern learning
# DB_HOST=postgres
# DB_PORT=5432
# DB_NAME=voicemail_detection
# DB_USER=freeswitch
# DB_PASSWORD=secure_password

# =============================================================================
# SECURITY
# =============================================================================

# Event Socket Password
EVENT_SOCKET_PASSWORD=ClueCon

# Default extension password
DEFAULT_EXTENSION_PASSWORD=452910

# =============================================================================
# DOCKER SETTINGS
# =============================================================================

# Container names
FREESWITCH_CONTAINER_NAME=freeswitch-voicemail-detector
VOSK_CONTAINER_NAME=vosk-asr-server

# Docker network
NETWORK_NAME=voicemail_detection_network