<!--
Usuario de Prueba para Sistema de Detección de Buzones
Extensión: 1001 - Agente de Prueba Voicemail Detection
-->
<include>
  <user id="1001">
    <params>
      <!-- Credenciales del Usuario -->
      <param name="password" value="1001"/>
      <param name="vm-password" value="1001"/>
      
      <!-- Configuración del Usuario -->
      <param name="accountcode" value="1001"/>
      <param name="user_context" value="default"/>
      <param name="effective_caller_id_name" value="VM Tester"/>
      <param name="effective_caller_id_number" value="1001"/>
      
      <!-- Configuración de Buzón de Voz -->
      <param name="vm-enabled" value="true"/>
      <param name="vm-mail-to" value="vmtester@localhost"/>
      <param name="vm-attach-file" value="true"/>
      <param name="vm-keep-local-after-email" value="true"/>
      
      <!-- Variables Especiales para Detección -->
      <param name="voicemail_detection_enabled" value="true"/>
      <param name="detection_agent" value="true"/>
      <param name="outbound_prefix" value="77751"/>
    </params>
    
    <variables>
      <!-- Variables de Usuario -->
      <variable name="toll_allow" value="domestic,international,local"/>
      <variable name="accountcode" value="1001"/>
      <variable name="user_context" value="default"/>
      <variable name="effective_caller_id_name" value="VM Detection Tester"/>
      <variable name="effective_caller_id_number" value="1001"/>
      
      <!-- Variables para Detección de Buzones -->
      <variable name="enable_voicemail_detection" value="true"/>
      <variable name="detection_timeout" value="30"/>
      <variable name="detection_confidence_threshold" value="85"/>
      <variable name="log_detection_calls" value="true"/>
      
      <!-- Variables de Gateway -->
      <variable name="default_gateway" value="voicemail_detection_gw"/>
      <variable name="outbound_auth_prefix" value="77751"/>
    </variables>
  </user>
</include>